module.config(function($stateProvider) {
  $stateProvider
    .state('exercisePlans', {
      url: '/exercise_plans',
      templateUrl: '<%= asset_path("exercise_plans/list.html") %>',
      controller: 'ExercisePlanListCtrl'
    })
    .state('exercisePlans.detail', {
      url: '/:id',
      templateUrl: '<%= asset_path("exercise_plans/detail.html") %>',
      controller: 'ExercisePlanDetailCtrl'
    });
});
